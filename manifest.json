{
	"manifest_version": 2,
	"version" : "1",
	"name": "Pixearch",
	"description": "Plugin for image based search with Computer Vision API recognition",
	"version": "1.0",
	"browser_action": {
		"default_popup": "index.html"
	},
	"icons":{
		"16" : "icon_16.png",
		"32" : "icon_32.png",
		"96" : "icon_96.png",
		"128": "icon_128.png"
	},
	"permissions": [
		"activeTab","tabs","contextMenus"
	],
	"background" :{
		"scripts" : ["scripts/jquery.js","scripts/contextMenu.js"],
		"persistent" : false
	}
}